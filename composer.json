{
    "name": "liinkiing/youtube-mp3-downloader-api",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2.5",
        "ext-amqp": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "antishov/doctrine-extensions-bundle": "^1.4",
        "aws/aws-sdk-php": "^3.128",
        "league/flysystem-aws-s3-v3": "^1.0",
        "oneup/flysystem-bundle": "^3.3",
        "ramsey/uuid-doctrine": "^1.5",
        "sensio/framework-extra-bundle": "^5.5",
        "spatie/regex": "^1.4",
        "symfony/console": "5.0.*",
        "symfony/dotenv": "5.0.*",
        "symfony/expression-language": "5.0.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.0.*",
        "symfony/framework-bundle": "5.0.*",
        "symfony/mercure-bundle": "^0.2.3",
        "symfony/messenger": "5.0.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/orm-pack": "^1.0",
        "symfony/process": "5.0.*",
        "symfony/profiler-pack": "^1.0",
        "symfony/security-bundle": "5.0.*",
        "symfony/serializer-pack": "^1.0",
        "symfony/string": "5.0.*",
        "symfony/translation": "5.0.*",
        "symfony/validator": "5.0.*",
        "symfony/yaml": "5.0.*"
    },
    "require-dev": {
        "symfony/debug-pack": "^1.0",
        "symfony/maker-bundle": "^1.14"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "heroku:deploy": "git push heroku master",
        "heroku:stop:web": "heroku ps:scale web=0",
        "heroku:stop:worker": "heroku ps:scale worker=0",
        "heroku:stop:all": "composer run heroku:stop:web && composer run heroku:stop:worker",
        "heroku:start:web": "heroku ps:scale web=1",
        "heroku:start:worker": "heroku ps:scale worker=1",
        "heroku:start:all": "composer run heroku:start:web && composer run heroku:start:worker",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.0.*"
        }
    }
}
